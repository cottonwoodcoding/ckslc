<h1>Welcome!</h1>
<hr />

<div class='row'>
  <div class="gallery">
    <% @photos.each do |photo| %>
      <div class='col s12 m4'>
        <a href="<%= photo.attachment.fullpath %>" data-caption="<%= photo.caption %>">
          <%= cl_image_tag(photo.attachment.path, { size: '200x200', crop: :thumb }) %>
        </a>
      </div>
    <% end %>
  </div>
</div>

<%= will_paginate @photos %>